{
  "log": {
    "level": "error",
    "output": "box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-word",
        "address": "https://hostvds-ru1.ydns.eu/dns-query",
        "address_resolver": "dns-resolve",
        "address_strategy": "prefer_ipv4",
        "detour": "direct"
      },
      {
        "tag": "dns-resolve",
        "address": "77.88.8.8",
        "address_strategy": "prefer_ipv4",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "action": "route",
        "server": "dns-word",
        "disable_cache": false,
        "outbound": "any"
      }
    ],
    "fakeip": {
      "enabled": false,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "final": "dns-word",
    "strategy": "prefer_ipv4",
    "independent_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "mtu": 9000,
      "address": "172.19.0.1/28",
      "auto_route": true,
      "strict_route": true,
      "endpoint_independent_nat": false,
      "stack": "system",
      "route_exclude_address": [
        "10.0.0.0/8",
        "100.64.0.0/10",
        "169.254.0.0/16",
        "172.16.0.0/12",
        "192.0.0.0/24",
        "192.0.2.0/24",
        "192.88.99.0/24",
        "192.168.0.0/16",
        "198.51.100.0/24",
        "203.0.113.0/24",
        "224.0.0.0/4",
        "255.255.255.255/32"
      ]
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "selector",
      "tag": "selector",
      "outbounds": [
        "Local-Dom",
        "HostingVDS-RU",
        "HostingVDS-RU-VM"
      ],
      "default": "HostingVDS-RU-VM"
    },
    {
      "type": "http",
      "tag": "Local-Dom",
      "server": "192.168.1.151",
      "server_port": 10808
    },
    {
      "type": "vless",
      "tag": "HostingVDS-RU",
      "server": "hostvds-ru.ydns.eu",
      "domain_strategy": "prefer_ipv4",
      "server_port": 443,
      "uuid": "e4cb6eca-18d4-48d5-9562-86a5e8740d07",
      "tls": {
        "enabled": true,
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        }
      },
      "transport": {
        "type": "httpupgrade"
      }
    },
    {
      "type": "vmess",
      "tag": "HostingVDS-RU-VM",
      "server": "hostvds-ru.ydns.eu",
      "domain_strategy": "prefer_ipv4",
      "server_port": 64358,
      "uuid": "c3255d49-443d-46cb-83d5-d8d88b816444",
      "security": "chacha20-poly1305",
      "alter_id": null,
      "transport": {
        "type": "httpupgrade"
      }
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "action": "resolve",
        "strategy": "prefer_ipv4",
        "server": "dns-word"
      },
      {
        "protocol": "QUIC",
        "action": "route",
        "outbound": "block"
      },
      {
       "port":
        [25, 465, 587, 993, 995, 18090, 22123, 59090],
        "action": "route",
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "domain": [
          "themoviedb.org",
          "tmbd.org",
          "tmdb-image-prod.b-cdn.net",
          "kino.pub"
        ],
        "action": "route",
        "outbound": "selector"
      },
      {
        "domain_suffix": [
          "themoviedb.org",
          "tmbd.org",
          "tmdb-image-prod.b-cdn.net",
          "kino.pub"
        ],
        "action": "route",
        "outbound": "selector"
      },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "action": "route",
        "outbound": "selector"
      },
      {
        "domain_suffix": [
          "ru",
          "su",
          "xn--",
          "yandex.net"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-ru",
          "gov-ru"
        ],
        "action": "route",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-ru",
        "format": "binary",
        "url": "https://github.com/malikshi/sing-box-geo/raw/rule-set-geoip/geoip-ru.srs",
        "download_detour": "direct",
        "update_interval": "1d"
      },
      {
        "type": "remote",
        "tag": "gov-ru",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/rule-set/geosite-category-ru.srs",
        "download_detour": "direct",
        "update_interval": "10d"
      },
      {
        "type": "remote",
        "tag": "geosite-google",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/rule-set/geosite-google.srs",
        "download_detour": "direct",
        "update_interval": "1d"
      },
      {
        "type": "remote",
        "tag": "geoip-google",
        "format": "binary",
        "url": "https://github.com/runetfreedom/russia-blocked-geoip/raw/refs/heads/release/srs/google.srs",
        "download_detour": "direct",
        "update_interval": "1d"
      }
    ],
    "final": "selector",
    "auto_detect_interface": true,
    "override_android_vpn": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_rdrc": true
    }
  }
}
