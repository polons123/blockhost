{
  "log": {
    "level": "error",
    "output": "box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-word",
        "address": "https://4vps-lv.ydns.eu/dns-query",
        "address_resolver": "dns-resolve",
        "address_strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns-ru",
        "address": "185.222.222.222",
        "address_strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns-resolve",
        "address": "185.222.222.222",
        "address_strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns-block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "domain_suffix": [
          "arpa.",
          "arpa"
        ],
        "server": "dns-block"
      },
      {
        "outbound": "any",
        "server": "dns-word"
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "strategy": "ipv4_only",
    "independent_cache": false,
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "direct",
      "tag": "dns-in",
      "listen": "::",
      "listen_port": 6450,
      "domain_strategy": "ipv4_only",
      "override_address": "46.8.232.156",
      "override_port": 853
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "mtu": 9000,
      "address": "172.19.0.1/28",
      "auto_route": true,
      "strict_route": true,
      "endpoint_independent_nat": true,
      "stack": "mixed",
      "route_exclude_address": [
        "10.0.0.0/8",
        "100.64.0.0/10",
        "169.254.0.0/16",
        "172.16.0.0/12",
        "192.0.0.0/24",
        "192.0.2.0/24",
        "192.88.99.0/24",
        "192.168.0.0/16",
        "198.51.100.0/24",
        "203.0.113.0/24",
        "224.0.0.0/4",
        "255.255.255.255/32"
      ],
      "sniff": true,
      "domain_strategy": "ipv4_only"
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "selector",
      "outbounds": [
        "HostingVDS-RU",
        "HostingVDS-RU-VM"
      ],
      "default": "HostingVDS-RU"
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "vless",
      "tag": "HostingVDS-RU",
      "server": "hostvds-ru.ydns.eu",
      "domain_strategy": "ipv4_only",
      "server_port": 443,
      "uuid": "e4cb6eca-18d4-48d5-9562-86a5e8740d07",
      "tls": {
        "enabled": true,
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        }
      },
      "transport": {
        "type": "httpupgrade"
      }
    },
    {
      "type": "vmess",
      "tag": "HostingVDS-RU-VM",
      "server": "hostvds-ru.ydns.eu",
      "domain_strategy": "ipv4_only",
      "server_port": 64358,
      "uuid": "c3255d49-443d-46cb-83d5-d8d88b816444",
      "security": "chacha20-poly1305",
      "alter_id": null,
      "transport": {
        "type": "httpupgrade"
      }
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "protocol": "quic",
        "outbound": "block"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "outbound": "selector"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-google",
        "format": "binary",
        "url": "https://github.com/SagerNet/sing-geosite/raw/rule-set/geosite-google.srs",
        "download_detour": "direct",
        "update_interval": "1d"
      },
      {
        "type": "remote",
        "tag": "geoip-google",
        "format": "binary",
        "url": "https://github.com/runetfreedom/russia-blocked-geoip/raw/refs/heads/release/srs/google.srs",
        "download_detour": "direct",
        "update_interval": "1d"
      }
    ],
    "final": "selector",
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_detour": "direct"
    }
  }
}
